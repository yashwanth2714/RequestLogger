{
    "name": "request-logger",
    "version": "1.0.0",
    "description": "A local request logging tool using ngrok. This tool writes request bodies to a file and provides a user interface built with React",
    "main": "index.js",
    "scripts": {
        "install-all": "npm i && cd client && npm i",
        "start": "npm-run-all --parallel start:server start:client",
        "start:server": "node index",
        "start:client": "timeout /t 7 /nobreak && npm run start --prefix client"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "localtunnel": "^2.0.2"
    },
    "devDependencies": {
        "npm-run-all": "^4.1.5"
    }
}
